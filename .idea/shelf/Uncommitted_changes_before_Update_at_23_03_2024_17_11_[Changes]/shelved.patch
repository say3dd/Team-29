blic function getRandomProducts(){\n        $randomProducts = Product::where('stock', '>', 0)\n            ->take(30)\n            ->get()\n            ->random(3);\n        return $randomProducts;\n    }\n\n    public function about(){\n        return view('FrontEnd.about');\n    }\n\n}\n
